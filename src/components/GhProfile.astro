<article id="gh-profile" />
<script>
  import { marked } from 'marked'

  const res = await fetch('https://raw.githubusercontent.com/sakkke/sakkke/main/README.md');
  const text = await res.text();
  const html = marked.parse(text);
  const elem = document.getElementById('gh-profile');
  elem.innerHTML = html;
</script>
